---
import BaseCenteredTitle from "../../components/BaseCenteredTitle.astro";

import Link from "../../components/react/Link";
import { LinkData } from "./linkData";

const title = "Links";
---

<BaseCenteredTitle title={title} lang="en">
  {
    LinkData.map((link) => (
      <Link
        url={link.url}
        icon={link.icon}
        title={link.title}
        description={link.description}
      />
    ))
  }
</BaseCenteredTitle>
